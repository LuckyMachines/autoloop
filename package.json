  "devDependencies": {
    "@chainlink/contracts": "^0.5.1",
    "@ethersproject/providers": "^5.4.7",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.2.0",
    "@types/mocha": "^9.1.0",
    "chai": "^4.2.0",
    "dotenv": "^16.0.3",
    "ethers": "^5.4.7",
    "hardhat": "^2.12.5",
    "hardhat-abi-exporter": "^2.10.1",
    "hardhat-contract-sizer": "^2.8.0",
    "hardhat-gas-reporter": "^1.0.8",
    "solidity-coverage": "^0.8.1",
    "solidity-docgen": "^0.6.0-beta.34",
    "ts-node": ">=8.0.0",
    "typechain": "^8.1.0",
    "typescript": ">=4.5.0"
  },
  "description": "This is a template to quickly spin up a hardhat project with some standard packages. The sample contract / test / deploy files are left here for reference, but should be deleted from any production project.",
  "main": "hardhat.config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "contracts": "npx hardhat run scripts/get-registered-contracts.js --network",
    "deploy": "npx hardhat run scripts/deploy.js --network",
    "register-contract": "npx hardhat run scripts/register-contract.js --network",
    "deregister-contract": "npx hardhat run scripts/deregister-contract.js --network",
    "deploy-sample": "npx hardhat run scripts/deploy-sample-game.js --network",
    "deposit": "npx hardhat run scripts/deposit.js --network",
    "register-sample": "npx hardhat run scripts/register-sample-game.js --network",
    "deregister-sample": "npx hardhat run scripts/deregister-sample-game.js --network",
    "sample-info": "npx hardhat run scripts/sample-game-info.js --network",
    "update-gas-buffer": "npx hardhat run scripts/update-gas-buffer.js --network",
    "test": "npx hardhat test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LuckyMachines/autoloop"
  },
  "keywords": [
    "hardhat",
    "blockchain",
    "template",
    "blockchain",
    "game"
